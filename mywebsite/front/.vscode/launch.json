    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/mywebsite-src-front",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--allow-net",
                "--importmap=import_map.json",
                "--inspect",
                "-A",
                "main.ts"
            ],
            "port": 9229,
            "console": "integratedTerminal"
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 9229
        }
    ]
}